{
    "version": 2,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 20
    },
    "configurePresets": [
        {
            "name": "build",
            "displayName": "Build for NRF5340 SENSEIv1 APP",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "NCS_TOOLCHAIN_VERSION": "NONE",
                "BOARD": "nrf5340_senseiv1_cpuapp",
                "CACHED_CONF_FILE": "${sourceDir}/prj.conf"
            }
        }
    ]
}